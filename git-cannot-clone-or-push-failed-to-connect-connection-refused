    StackExchange.ready(function () {

        StackExchange.Hero.init("stk", "a");

        var location = 0;
        if ($("body").hasClass("questions-page")) {
            location = 1;
        } else if ($("body").hasClass("question-page")) {
            location = 1;
        } else if ($("body").hasClass("faq-page")) {
            location = 5;
        } else if ($("body").hasClass("home-page")) {
            location = 3;
        }

        $('#tell-me-more').click(function () {
            StackExchange.using("gps", function () {
                StackExchange.gps.track("hero.action", { hero_action_type: 'cta', location: location }, true);
            });
        });

        $('.js-dismissable-hero #close').click(function () {
            StackExchange.using("gps", function () {
                StackExchange.gps.track("hero.action", { hero_action_type: "minimize", location: location }, true);
              });

            StackExchange.Hero.dismiss();

            $.ajax({
                url: "/hero-mini",
                success: function (data) {
                    $(".js-dismissable-hero").fadeOut("fast", function () {
                        $(".js-dismissable-hero").replaceWith(data);
                        $("#herobox-mini").fadeIn("fast");
                    });
                }
            });
            return false;
        });
    });
</script>    <div class="inner-content clearfix">
            <div id="question-header">
                    <h1 itemprop="name"><a href="/questions/24543372/git-cannot-clone-or-push-failed-to-connect-connection-refused" class="question-hyperlink">git cannot clone or push? failed to connect, connection refused</a></h1>

            
<div class="pl8 aside-cta" role="navigation" aria-label="ask new question">
        <a href="/questions/ask" class="ws-nowrap btn-outlined" >Ask Question</a>
